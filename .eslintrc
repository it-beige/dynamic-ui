{
  "globals": {
    "ga": true,
    "chrome": true,
    "Promise": true,
    "Reflect": true,
    "Map": true
  },
  "plugins": ["html", "json"],
  "extends": "elemefe",
  "rules": {
    "no-restricted-globals": ["error", "event", "fdescribe"],
    "no-spaced-func": 2 //函数调用时 函数名与()之间不能有空格
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
