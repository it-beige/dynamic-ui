{
  "globals": {
    "ga": true,
    "chrome": true,
    "Promise": true,
    "Reflect": true,
    "Map": true
  },
  "plugins": ["html", "json"],
  "extends": "elemefe",
  "rules": {
    "no-restricted-globals": ["error", "event", "fdescribe"],
    // 函数定义时括号前面要不要有空格
    "space-before-function-paren": [2, "always"]
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
